{
  "name": "@telnyx/cli",
  "version": "2.0.0",
  "description": "Command-line interface for Telnyx APIs - manage phone numbers, messaging, voice calls, and more",
  "main": "bin/telnyx",
  "bin": {
    "telnyx": "./bin/telnyx"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "telnyx",
    "cli",
    "api",
    "voice",
    "messaging",
    "sms",
    "mms",
    "phone-numbers",
    "voip",
    "telephony",
    "fax",
    "sip",
    "2fa",
    "verification"
  ],
  "author": "Telnyx <support@telnyx.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-telnyx/telnyx-cli.git"
  },
  "homepage": "https://developers.telnyx.com",
  "bugs": {
    "url": "https://github.com/team-telnyx/telnyx-cli/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "bin/",
    "src/api/",
    "src/commands/",
    "src/config/",
    "src/ui/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.13.4",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "conf": "^10.2.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "jest": "^30.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/*.test.js"
    ]
  }
}
